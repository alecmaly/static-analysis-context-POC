        "highlight": ""
    },
    {
        "name": "external state changing functions",
        "include": "(?=游눤)",
        "exclude": "(slither|游릴|constructor)",
        "highlight": ">out 0"
    },
    {
        "name": "internal state changing functions",
        "include": "",
        "exclude": "(游눤|slither|游릴|constructor)",
        "highlight": ">out 0"
    },
    {
        "name": "Scope: (in scope, no interfaces)",
        "include": "(?=游꿢)",
