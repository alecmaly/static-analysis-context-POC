    {
        "name": "highlight entry nodes",
        "include": "<no_change>",
        "exclude": "<no_change>",
        "highlight": "<entry>"
    },
    {
        "name": "highlight exit nodes",
        "include": "<no_change>",
        "exclude": "<no_change>",
        "highlight": "<exit>"
